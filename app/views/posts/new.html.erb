<%= form_with model: @post do |f| %>
  <div class="mb-3">
    <%= f.label "Title", for: "post-title", class: "form-label" %>
    <%= f.text_field type:"text", class: "form-control", id: "post-title"%>
  </div>
  <div class="mb-3">
    <%= f.label "Body", for: "post-body", class: "form-label" %>
    <%= f.text_area type:"text", class: "form-control", id: "post-body"%>
  </div>
  <%= f.button "Submit", type:"submit", class: "btn btn-primary" %>
<% end %>
